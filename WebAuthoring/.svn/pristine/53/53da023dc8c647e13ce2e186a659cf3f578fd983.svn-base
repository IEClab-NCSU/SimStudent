.separator_vertical
{
	margin-top: 4px;
	margin-bottom: 1px;
	margin-left: 4px;
	margin-right: 4px;
	border:1px inset #ffffff;
	height:18px; 
	float: left;
}

.iconbutton 
{
	margin-top: 2px;
	margin-bottom: 2px;
	margin-left: 2px;
	margin-right: 2px;
	padding: 1px;
	/*background-color:#f9f9f9;*/
	text-indent:0;
	border:2px solid #dcdcdc;
	color:#666666;
	font-family:Verdana;
	font-size:11px;
	font-weight:bold;
	font-style:normal;
	height:18px;
	line-height:18px;
	width:18px;
	text-decoration:none;
	text-align:center;
	text-shadow:1px 1px 0px #ffffff;
	float: left;
}

.iconbutton:hover 
{
	/*background-color:#9e9b9e;*/
	background-color:#f9f9f9;
}

.iconbutton:active 
{
	position:relative;
	top:1px;
}

.grid {
    border: 1px solid #ccc;
    border-width: 1px 0 0 1px;
}

.grid div {
    border: 1px solid #ccc;
    border-width: 0 1px 1px 0;
    float: left;
}

