.silex-workspace
  .silex-popup-msg.hidden
  
  //logo
  div#ctat-logo
  
  // menu
  .silex-menu

  .silex-context-menu
    #context-menu-save.button.save.off(title='Save')
    #context-menu-undo.button.undo.off(title='Undo')
    #context-menu-redo.button.redo.off(title='Redo')
    .button.sep
    #context-menu-delete.button.delete.off(title='Delete')
    #context-menu-copy.button.copy.off(title='Copy')
    #context-menu-paste.button.paste.off(title='Paste')
    .button.sep
    #context-menu-top.button.top.off(title='Bring to front')
    #context-menu-up.button.up.off(title='Bring forward')
    #context-menu-down.button.down.off(title='Send backward')
    #context-menu-bottom.button.bottom.off(title='Send to back')
    .button.sep
    #context-menu-js-file.button.js.off(title="Import JS file")
    #context-menu-css-file.button.css.off(title="Import CSS file")
    .button.sep
    #context-menu-grid.button.grid(title="Disable Snap-to-Grid")
    #context-menu-lasso.button.lasso.off(title="Lasso tool")
    span.context-menu-float
      #context-menu-demonstrate.button.demonstrate(title='Launch Interface')
      a#context-menu-help.button.help(href='https://github.com/CMUCTAT/CTAT/wiki', title="get help here!", target='_blank')

  // stage
  .silex-stage
    // notranslate is to prevent chrome automatic translation
    iframe#silex-stage-iframe.silex-stage-iframe.notranslate
    
  .ctatstatusbar
        span#silex-statusbar-status-field.statusbar-field
        span#silex-statusbar-filename-field.statusbar-field
        span#silex-statusbar-type-field.statusbar-field
        span#silex-statusbar-cursor-field.statusbar-field
            |  X: 
            span#editor-xpos
            |  Y: 
            span#editor-ypos
  
  // properties tool
  .property-tool-container
    .silex-property-tool.notranslate
      include ./property-tool.jade
  
  // component palette
  include ./component-palette.jade
  
  // dialogs background
  .dialogs-background

  // settings
  .silex-settings-dialog.hidden-dialog
    include ./settings-dialog.jade